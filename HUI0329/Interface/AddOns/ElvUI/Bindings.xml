<Bindings>
  <Binding name="Raid Marker" category="BINDING_HEADER_ELVUI" header="ELVUI" runOnUp="true">
    RaidMark_HotkeyPressed(keystate)
  </Binding>
   <Binding name="Farm Mode" category="BINDING_HEADER_ELVUI" runOnUp="false">
    FarmMode()
  </Binding> 
   <Binding name="Toggle Chat (Left)" category="BINDING_HEADER_ELVUI" runOnUp="false">
    HideLeftChat()
  </Binding> 
   <Binding name="Toggle Chat (Right)" category="BINDING_HEADER_ELVUI" runOnUp="false">
    HideRightChat()
  </Binding> 
   <Binding name="Toggle Chat (Both)" category="BINDING_HEADER_ELVUI" runOnUp="false">
    HideBothChat()
  </Binding>   
</Bindings>